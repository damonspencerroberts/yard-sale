<% @bookings.each do |booking|%>
	<div class="card">
	 	<p><%= booking.backyard.name %></p>
	 	<p><%= booking.backyard.address %></p>
	 	<p><%= booking.backyard.price %></p>
		<p><%= booking.date %> - <%= booking.number_of_guests %></p>
		<%= link_to "delete", backyard_booking(@booking), method: :delete, data: {confirm: "Are you sure?"} %>
	</div>
<% end %>